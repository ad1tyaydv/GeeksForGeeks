class Solution{
public:
	int countZeros(vector<vector<int>>A)
	{
	    int count = 0;
	    int n = A.size();
	    int m = A[0].size();
	    
	    for(int i=0;i<n;i++) {
	        for(int j=0;j<m;j++) {
	            if(A[i][j] == 0) {
	                count++;
	            }
	        }
	    }
	    
	    return count;
	}
};
